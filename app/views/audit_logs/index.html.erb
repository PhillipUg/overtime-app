<h1>Audit Logs Dashboard</h1>

<table class="table table-sm table-hover table-striped">
  <thead>
    <tr>
      <th>
        #
      </th>
      <th>
        Employee
      </th>
      <th>
        Week Starting
      </th>
      <th>
        Comfirmed At
      </th>
      <th>
        Status
      </th>
    </tr>
  </thead>
  <tbody id='audit_logs'>
    <%= render @audit_logs %>
  </tbody>
</table>
<div id="paginator">
  <%= paginate @audit_logs, remote: true %>
</div>